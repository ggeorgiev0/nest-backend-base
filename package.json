{
  "name": "nest-backend-base",
  "version": "0.0.1",
  "description": "A robust NestJS backend following Domain Driven Design architecture principles",
  "author": "Georgi Georgiev",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./test/jest-config.json",
    "test:watch": "jest --config ./test/jest-config.json --watch",
    "test:cov": "jest --config ./test/jest-config.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest-config.json --runInBand",
    "test:unit": "jest --config ./test/jest-config.json --group=unit",
    "test:integration": "jest --config ./test/jest-config.json --group=integration",
    "test:unit:watch": "jest --config ./test/jest-config.json --group=unit --watch",
    "test:integration:watch": "jest --config ./test/jest-config.json --group=integration --watch",
    "test:all": "jest --config ./test/jest-config.json --runInBand",
    "test:all:parallel": "jest --config ./test/jest-config.json",
    "prepare": "husky && bash ./scripts/git-hooks/install-hooks.sh",
    "hooks:install": "bash ./scripts/git-hooks/install-hooks.sh",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "supabase": "supabase"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@supabase/supabase-js": "2.49.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "nestjs-pino": "4.4.0",
    "pino": "9.6.0",
    "pino-http": "10.4.0",
    "pino-pretty": "13.0.0",
    "pino-roll": "^2.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "supabase": "2.19.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@prisma/client": "6.5.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^57.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-docblock": "^29.7.0",
    "jest-runner": "^29.7.0",
    "jest-runner-groups": "2.2.0",
    "jest-watch-typeahead": "2.2.2",
    "lint-staged": "^15.5.0",
    "prettier": "^3.4.2",
    "prisma": "6.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "webpack": "^5.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
